
@{
    ViewData["Title"] = "EmployeeList";
}

<h1>Employee List</h1>

<table class="table table-borered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Salary</th>
    </tr>
    @foreach (Employee e in ViewBag.Employees)
    {
        <tr>
            <td>@e.Id</td>
            <td>@e.Name</td>
            <td>@e.Salary</td>
        </tr>
    }
</table>